{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "Policy": {
    "roles": [
      {
        "name": "Admin",
        "subjects": [ "simple_client_admin" ]
      },
      {
        "name": "User",
        "subjects": [ "simple_client_admin", "simple_client" ]
      }
    ]
  }
}
